generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DB_URL")
}

model Productos {
    id String @id @default(uuid())
    name String
    price Float
    stock Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("productos")
}